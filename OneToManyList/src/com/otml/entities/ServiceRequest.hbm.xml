<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.otml.entities">
	<class name="ServiceRequest" table="SERVICE_REQUEST">
		<id name="serviceRequestNo" column="SERVICE_REQUEST_NO">
			<generator class="increment" />
		</id>
		<property name="problemDescription" column="PROBLEM_DESCR" />
		<property name="priority" column="PRIORITY" />
		<property name="eta" column="ETA" />
		<property name="serviceRequestDate" column="SERVICE_REQUEST_DT" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>